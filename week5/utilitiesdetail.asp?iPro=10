<?php
// Permanent redirection
$q = 0;
if (!empty($_REQUEST['iPro'])) {
	$q = (int) $_REQUEST['iPro'];
	PermanentRedirectPage($q);
}else
{
	header("HTTP/1.1 301 Moved Permanently");
	header("Location: http://mycplus.com/category/free-utilities/");
}
exit();

